# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_project_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# OpenAI Configuration (for embeddings and chat)
OPENAI_API_KEY=your_openai_api_key

# OpenRouter Configuration (optional embeddings provider)
OPENROUTER_API_KEY=your_openrouter_api_key
OPENROUTER_APP_NAME=Universal IDE Platform
OPENROUTER_APP_URL=https://your-app-domain.com

# Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:54322/postgres

# Application Configuration
NEXT_PUBLIC_APP_URL=http://localhost:3000
NODE_ENV=development

# OAuth Configuration (Google)
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id
NEXT_PUBLIC_GOOGLE_CLIENT_SECRET=your_google_client_secret

# Monitoring & Logging
# Optional: Sentry DSN for error tracking in production
SENTRY_DSN=
NEXT_PUBLIC_SENTRY_DSN=

# Optional: API key for centralized logging service
LOGS_API_KEY=

# Optional: Datadog configuration
DATADOG_API_KEY=
NEXT_PUBLIC_DATADOG_APP_ID=
NEXT_PUBLIC_DATADOG_CLIENT_TOKEN=

# Optional: Mixpanel configuration
MIXPANEL_TOKEN=
NEXT_PUBLIC_MIXPANEL_TOKEN=

# Optional: UptimeRobot heartbeat URL for availability monitoring
UPTIMEROBOT_HEARTBEAT_URL=

# Cron Jobs
# Secret token for authenticating cron job requests
# Generate a secure random string for production
CRON_SECRET=your-secure-cron-secret-token

# Optional: Slack webhook URL for failure notifications
SLACK_WEBHOOK_URL=

# Optional: Admin email for failure notifications
ADMIN_EMAIL=
