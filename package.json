{
  "name": "universal-ide-database",
  "version": "1.0.0",
  "description": "Database schema and utilities for Universal IDE platform with Supabase",
  "main": "lib/supabase-client.ts",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:seed": "supabase db seed",
    "db:generate-types": "supabase gen types typescript --local > types/generated.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "cheerio": "^1.0.0-rc.12",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "robots-parser": "^3.0.1",
    "tiktoken": "^1.0.22",
    "turndown": "^7.1.2",
    "typescript": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/turndown": "^5.0.4",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
